<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheet/styles.css" />
  </head>
  <body>
    <% if (locals.errors) {%>
    <ul class="errorMessageContainer">
      <% errors.forEach(function(error) { %>
      <li class="errorMessage"><%= error.msg %></li>
      <% }); %>
    </ul>
    <% } %>
  </body>
</html>
